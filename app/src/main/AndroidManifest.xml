<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cloudymous.visitgowa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BerandaActivity" />
        <activity
            android:name=".Activity_ListFood"
            android:label="@string/food"
            android:theme="@style/DefaultMenu" />
        <activity
            android:name=".Activity_ListNature"
            android:label="@string/nature"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />
        <activity
            android:name=".Activity_ListHeritage"
            android:label="@string/heritage"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
        <activity
            android:name=".Activity_ListSport"
            android:label="@string/sport"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />
    </application>

</manifest>